<p id="notice"><%= notice %></p>

<h1>Listing Course Course Data</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_course_data.each do |course_course_datum| %>
      <tr>
        <td><%= course_course_datum.data %></td>
        <td><%= link_to 'Show', course_course_datum %></td>
        <td><%= link_to 'Edit', edit_course_course_datum_path(course_course_datum) %></td>
        <td><%= link_to 'Destroy', course_course_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course datum', new_course_course_datum_path %>
